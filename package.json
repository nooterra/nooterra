    "demo:mcp-paid-llm": "node scripts/demo/mcp-paid-exa.mjs --workload=llm",
    "demo:compositional-settlement": "node scripts/demo/compositional-settlement-3hop.mjs",
    "demo:ui:prep": "node scripts/demo/delivery-robot/export-ui-fixture.mjs",
    "demo:ui": "cd dashboard && npm run dev",
    "demo:build": "cd dashboard && npm run build",
    "pilot:finance-pack": "PROXY_DETERMINISTIC_IDS=1 node scripts/pilot/finance-pack.mjs",
    "finance:bundle": "node scripts/finance-pack/bundle.mjs",
    "proof:job": "node scripts/proof-bundle/job.mjs",
    "proof:month": "node scripts/proof-bundle/month.mjs",
    "trust:wizard": "node scripts/trust-config/wizard.mjs",
    "keys:rotate": "node scripts/trust-config/rotate-settld-pay.mjs",
    "scaffold:paid-tool": "node scripts/scaffold/create-settld-paid-tool.mjs",
    "settlement:x402:batch": "node scripts/settlement/x402-batch-worker.mjs",
    "mcp:server": "node scripts/mcp/settld-mcp-server.mjs",
    "mcp:probe": "node scripts/mcp/probe.mjs",
    "mcp:http": "node scripts/mcp/settld-mcp-http-gateway.mjs",
    "provider:conformance": "node scripts/provider/conformance-run.mjs",
    "provider:publish": "node scripts/provider/publish.mjs",
    "provider:keys:generate": "node scripts/provider/keys-generate.mjs",
    "governance:export": "node scripts/governance/export.mjs",
    "openapi:write": "node scripts/openapi/write.mjs",
    "sdk:smoke": "node scripts/sdk/smoke.mjs",
    "sdk:smoke:py": "PYTHONDONTWRITEBYTECODE=1 python3 scripts/sdk/smoke-python.py",
    "sdk:first-run": "node scripts/examples/sdk-first-verified-run.mjs",
