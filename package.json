{
  "name": "settld",
  "version": "0.2.0",
  "description": "Settld kernel CLI and local control-plane tooling",
  "private": false,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aidenlippert/settld"
  },
  "files": [
    "SETTLD_VERSION",
    "README.md",
    "Dockerfile",
    "docker-compose.yml",
    "bin",
    "conformance",
    "packages/artifact-verify/src",
    "scripts",
    "docs",
    "src",
    "services/finance-sink",
    "services/x402-gateway",
    "services/receiver"
  ],
  "bin": {
    "settld": "bin/settld.js",
    "settld-mcp": "bin/settld-mcp"
  },
  "exports": {
    "./mcp": "./scripts/mcp/settld-mcp-server.mjs"
  },
  "scripts": {
    "postinstall": "node scripts/init/postinstall-sanity.mjs",
    "dev:api": "node src/api/server.js",
    "dev:maintenance": "node src/api/maintenance.js",
    "dev:receiver": "node services/receiver/src/server.js",
    "dev:finance-sink": "node services/finance-sink/src/server.js",
    "dev:magic-link": "node services/magic-link/src/server.js",
    "dev:x402-gateway": "node services/x402-gateway/src/server.js",
    "quickstart:x402": "node scripts/quickstart/x402.mjs",
    "start:prod": "NODE_ENV=production node src/api/server.js",
    "start:maintenance": "NODE_ENV=production node src/api/maintenance.js",
    "start:receiver": "NODE_ENV=production node services/receiver/src/server.js",
    "start:finance-sink": "NODE_ENV=production node services/finance-sink/src/server.js",
    "start:magic-link": "NODE_ENV=production node services/magic-link/src/server.js",
    "start:x402-gateway": "NODE_ENV=production node services/x402-gateway/src/server.js",
    "agent:sim": "node src/agent/agent-sim.js",
    "chaos": "node scripts/chaos.js",
    "load:ingest": "k6 run scripts/load/ingest-burst.k6.js",
    "load:deliveries": "node scripts/load/run-delivery-load.js",
    "acceptance:full-stack": "bash scripts/acceptance/full-stack.sh",
